<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.transport.system.model.Ticket" table="ticket" schema="transportdatabase">
        <id name="ticketId">
            <column name="ticket_id" sql-type="int(11)"/>
        </id>
        <property name="departureStationId">
            <column name="departure_station_id" sql-type="int(11)"/>
        </property>
        <property name="arrivalStationId">
            <column name="arrival_station_id" sql-type="int(11)"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="int(11)"/>
        </property>
        <property name="trainId">
            <column name="train_id" sql-type="int(11)"/>
        </property>
        <many-to-one name="stationByDepartureStationId" class="com.transport.system.model.Station">
            <column name="departure_station_id"/>
        </many-to-one>
        <many-to-one name="stationByArrivalStationId" class="com.transport.system.model.Station">
            <column name="arrival_station_id"/>
        </many-to-one>
        <many-to-one name="userByUserId" class="com.transport.system.model.User">
            <column name="user_id"/>
        </many-to-one>
        <many-to-one name="trainByTrainId" class="com.transport.system.model.Train">
            <column name="train_id"/>
        </many-to-one>
    </class>
</hibernate-mapping>